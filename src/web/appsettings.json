{
  "ConnectionStrings": {
    "Default": "Host=hostIP;Port=5432;Username=postgres;Password=pwd;Database=aspcore-template"
  },
  "Authentication": {
    "Google": {
      "ClientId": "#{auth.google.clientid}",
      "ClientSecret": "#{auth.google.clientsecret}"
    }
  },
  "Jwt": {
    "Key": "#{jwt.key}",
    "Issuer": "#{jwt.issuer.guid}",
    "ExpireDays": 30
  },
  "Logging": {
    "IncludeScopes": true,
    "LogLevel": {
      "Default": "Error",
      "Hangfire": "Error"
    }
  }
}